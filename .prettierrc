{
  "printWidth": 80,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "overrides": [
      {
          "files": "*.py",
          "options": {
              "printWidth": 80,
              "tabWidth": 4,
              "useTabs": false,
              "singleQuote": false
          }
      },
      {
          "files": "*.ipynb",
          "options": {
              "printWidth": 100
          }
      }
  ]
}